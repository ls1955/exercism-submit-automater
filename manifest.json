{
    "manifest_version": 3,
    "name": "Exercism Submit Automater",
    "description": "Click three buttons for you during submission via hotkey",
    "version": "1.0.0",
    "icons": {
        "16": "images/icon-16.png"
    },
    "background": {
        "service_worker": "buttons_clicker.js"
    },
    "action": {
        "default_icons": {
            "16": "images/icon-16.png"
        }
    },
    "permissions": ["activeTab", "scripting"]
}
